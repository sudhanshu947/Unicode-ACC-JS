{
    "version": 2,
    "name": "license-key-generator",
    "builds": [
      { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/public/$1" },
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/public/index.html" }
    ],
    "github": {
      "silent": true
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      }
    ]
  }